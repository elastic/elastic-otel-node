{
  "name": "elastic-otel-node-examples",
  "license": "Apache-2.0",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "clean": "rm -rf node_modules",
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:deps",
    "lint:except-eslint-and-prettier": "npm run lint:deps",
    "lint:eslint": "eslint .",
    "lint:prettier": "(cd ../; prettier --check $npm_config_local_prefix)",
    "lint:deps": "dependency-check '*.js' '*.mjs' -i pino-pretty  -i @elastic/opentelemetry-node -i bunyan  # '-i bunyan' should not be necessary, dep-check cannot handle ESM?",
    "dev:use-local-edot-nodejs": "npm install --no-save ../packages/opentelemetry-node"
  },
  "dependencies": {
    "@elastic/opentelemetry-node": "*",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.68.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "bunyan": "^1.8.15",
    "express": "^5.2.1",
    "pino-pretty": "^13.1.3"
  }
}
