{
  "compilerOptions": {
    // A default tsconfig (from 'tsc --init') except:
    // - "module" was changed from the default "commonjs" for types from the
    //   ".../sdk" entry-point to be found.
    "target": "es2016",
    "module": "node16",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
